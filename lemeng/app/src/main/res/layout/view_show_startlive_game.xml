<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_marginBottom="10px"
        android:visibility="gone"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/audio_player"
            android:layout_width="60px"
            android:layout_height="60px"
            android:src="@mipmap/audio_player"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15px"
            />

    </com.zhy.autolayout.AutoRelativeLayout>

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/rl_share"
        android:layout_width="match_parent"
        android:layout_marginBottom="10px"
        android:visibility="gone"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/iv_live_share"
            android:layout_width="60px"
            android:layout_height="60px"
            android:src="@mipmap/mg_room_btn_fen"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15px"
            />

    </com.zhy.autolayout.AutoRelativeLayout>

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="400px"
        android:background="@color/transparent_blank"
        >
      <com.zhy.autolayout.AutoLinearLayout
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:orientation="horizontal"
          android:layout_marginTop="10px"
          >

          <ImageView
              android:id="@+id/input_content"
              android:layout_width="60px"
              android:layout_height="60px"
              android:layout_marginLeft="19px"
              android:src="@mipmap/mg_room_btn_xinxi" />

          <ImageView
              android:id="@+id/iv_ranking"
              android:layout_width="60px"
              android:layout_height="60px"
              android:layout_marginLeft="20px"
              android:src="@mipmap/mg_room_btn_ranking" />
      </com.zhy.autolayout.AutoLinearLayout>
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_marginTop="10px"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="16px"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_live_mei"
                android:layout_width="60px"
                android:layout_height="60px"
                android:layout_marginBottom="19px"
                android:background="@mipmap/room_btn_meiyan_h"
                android:visibility="gone" />

            <com.zhy.autolayout.AutoRelativeLayout
                android:layout_width="60px"
                android:layout_height="60px"
                android:layout_marginBottom="19px"
                android:layout_marginLeft="10px"
                android:visibility="gone">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@mipmap/mg_room_btn_xin"
                    android:visibility="gone" />

                <ImageView
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:contentDescription="@string/empty"
                    android:src="@drawable/nim_main_tab_new_message_notify"
                    android:visibility="gone" />

                <com.netease.nim.uikit.common.ui.drop.DropFake
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="30px"
                    android:layout_marginRight="30px"
                    android:singleLine="true"
                    android:textColor="@android:color/white"
                    android:textSize="10sp"
                    android:visibility="gone" />
            </com.zhy.autolayout.AutoRelativeLayout>

            <ImageView
                android:layout_width="50px"
                android:layout_height="50px"
                android:contentDescription="@string/empty"
                android:visibility="gone"
                android:src="@mipmap/audio_player" />

            <ImageView
                android:layout_width="60px"
                android:layout_height="60px"
                android:contentDescription="@string/empty"
                android:src="@mipmap/game" />



            <com.zhy.autolayout.AutoRelativeLayout
                android:layout_width="60px"
                android:layout_height="60px"
                android:layout_marginLeft="30px">

                <ImageView
                    android:id="@+id/iv_live_privatechat"
                    android:layout_width="60px"
                    android:layout_height="60px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@mipmap/mg_room_btn_xin" />

                <ImageView
                    android:id="@+id/tab_new_indicator"
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:contentDescription="@string/empty"
                    android:src="@drawable/nim_main_tab_new_message_notify"
                    android:visibility="gone" />

                <com.netease.nim.uikit.common.ui.drop.DropFake
                    android:id="@+id/tab_new_msg_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="30px"
                    android:layout_marginRight="30px"
                    android:singleLine="true"
                    android:textColor="@android:color/white"
                    android:textSize="10sp"
                    android:visibility="gone" />
            </com.zhy.autolayout.AutoRelativeLayout>
            <ImageView
                android:id="@+id/iv_live_switch"
                android:layout_width="60px"
                android:layout_height="60px"
                android:layout_marginLeft="30px"
                android:background="@mipmap/room_btn_switchcamera" />
            <ImageView
                android:layout_width="50px"
                android:layout_height="50px"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/mg_room_btn_xin"
                android:visibility="gone" />

            <ImageView
                android:layout_width="60px"
                android:layout_height="60px"
                android:layout_marginBottom="19px"
                android:layout_marginLeft="15px"
                android:visibility="gone"
                android:background="@mipmap/mg_room_btn_fen" />
            <ImageView
                android:id="@+id/game_more_btn"
                android:layout_width="60px"
                android:layout_height="60px"
                android:layout_marginBottom="19px"
                android:layout_marginLeft="30px"
                android:background="@mipmap/more_btn" />
            <ImageView
                android:layout_width="50px"
                android:layout_height="50px"
                android:layout_marginBottom="19px"
                android:layout_marginLeft="15px"
                android:visibility="gone"
                android:src="@mipmap/close_white" />

        </com.zhy.autolayout.AutoLinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_centerVertical="true"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_audio_open"
                android:layout_marginLeft="20px"
                />

        </LinearLayout>
    </com.zhy.autolayout.AutoRelativeLayout>
</com.zhy.autolayout.AutoLinearLayout>